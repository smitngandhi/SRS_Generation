

# Use one:
# LLM_PROVIDER=groq
# LLM_PROVIDER=gemini


# LLM_MODEL=

# -----------------------------
# Groq (Groq-hosted models)
# -----------------------------
GROQ_API_KEY=your_groq_api_key_here

# Set to "auto" to use the first model available for your API key:
GROQ_MODEL=auto
#
# Or choose an explicit model id if you know you have access (examples):



# Gemini config supports GEMINI_API_KEY (preferred) and GOOGLE_API_KEY.
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-1.5-flash
